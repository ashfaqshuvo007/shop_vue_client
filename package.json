  "name": "solfactory-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@progress/kendo-ui": "^2019.3.927",
    "autolinker": "^3.11.1",
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "date-fns": "^2.2.1",
    "easy-affix": "^1.0.6",
    "emoji-js": "^3.4.1",
    "escape-goat": "^2.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "fecha": "^3.0.3",
    "file-loader": "^1.1.11",
    "filepond": "^4.7.1",
    "filepond-plugin-file-validate-type": "^1.2.4",
    "filepond-plugin-image-preview": "^4.2.1",
    "filepond-plugin-image-resize": "^2.0.4",
    "filepond-plugin-image-transform": "^3.3.2",
    "img-loader": "^3.0.1",
    "libphonenumber-js": "^1.7.23",
    "lingallery": "^1.0.26",
    "msgdown": "^1.0.2",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "register-service-worker": "^1.6.2",
    "v-tooltip": "^2.0.2",
    "vee-validate": "^2.2.11",
    "vue": "^2.6.10",
    "vue-affix": "^0.5.2",
    "vue-carousel": "^0.18.0",
    "vue-filepond": "^5.1.2",
    "vue-gallery-slideshow": "^1.3.1",
    "vue-phone-number-input": "^0.2.0",
    "vue-router": "^3.0.3",
    "vue-session": "^1.0.0",
    "vue-slider-component": "^3.0.32",
    "vue-social-sharing": "^2.4.6",
    "vue-tel-input": "^2.3.0",
    "vue-toasted": "^1.1.27",
    "vue-uuid": "^1.1.1",
    "vue2-filters": "^0.7.2",
    "vue2-transitions": "^0.3.0",
    "vuetify": "^2.0.12",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@mdi/font": "^4.4.95",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-e2e-nightwatch": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "material-design-icons-iconfont": "^5.0.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
